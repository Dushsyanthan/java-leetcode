class Solution {
  public static boolean isPalindrome(int x) {
	      if(x<0) {
	    	  return false;
	      }  
	      int temp=x;
	      Integer check=0;
	     while(temp>0) {
	    	 check=(check*10)+temp%10;
	    	 temp=temp/10;
	     }
	     if(check.equals(x)) {
	    	 return true;
	     }
	      return false;
	 }
}
