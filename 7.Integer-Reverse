class Solution {
   
	public static int reverse(int x) {
	    int ans=0;
	    int temp=(x>0)?x:(-1)*x;
	    	   	while(temp>0) {
                      if(ans>(Integer.MAX_VALUE-temp%10)/10){
                        return 0;
                      }
	    	   		ans=(ans*10)+(temp%10);
	    	   	//	System.out.println(ans);
	    	   		temp=temp/10;
	    	   	}
	    	   return   (x<0)?(-1)*ans:ans;
	 }
}
